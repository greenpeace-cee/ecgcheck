<?xml version="1.0"?>
<extension key="ecgcheck" type="module">
  <file>ecgcheck</file>
  <name>ECG Check</name>
  <description>Check email listed according to &#xA7; 7 E-Commerce-Gesetz (ECG)</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Patrick Figel</author>
    <email>pfigel@greenpeace.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/greenpeace-cee/ecgcheck</url>
    <url desc="Documentation">https://github.com/greenpeace-cee/ecgcheck</url>
    <url desc="Support">https://github.com/greenpeace-cee/ecgcheck/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-01-07</releaseDate>
  <version>1.0.0-beta.2</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Ecgcheck</namespace>
    <format>23.02.1</format>
    <angularModule>crmEcgcheck</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Ecgcheck_Upgrader</upgrader>
</extension>
